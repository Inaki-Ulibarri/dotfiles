(write ":D")
(format "~a" (+ 1 1))
(define (sum1 n) (+ n 1))
(format "~a" (sum1 3))
(empty-stream "abd")
(empty "a")
(empty? "abd")
(display (if (= 1 2) (begin 3) (5)))
(display (if (= 1 2) (begin 3) (begin 5)))

(define (sign n) (if (>= n 0) 'pos 'neg))
(sign 10)
(sign (-1))
(sign -1)
(symbol->string (sign -1))
(display "~a" (cond [(= 1 2) #t] [else #f]))
(display "~a" (cond [(= 1 2) 1] [else 2]))
(display "~a" (cond [(= 1 2) "1"] [else "2"]))
