\documentclass{article}
\usepackage[spanish]{babel}
\usepackage{amsmath, amssymb, amsthm}
\newtheorem{theorem}{Teorema}
\newtheorem{prop}{Proposición}
\usepackage[T1]{fontenc}

\title{Ejercicios Combinados}
\date{\today}
\author{Iñaki Ulibarri Utrilla}


\begin{document}
\maketitle \newpage

\begin{itemize}
\item Dados $Z_1 = 8(cos55° + isen55°)$, $Z_2 = -10 - 4i$, $Z_3 = 2cis73°$, $Z_4 = 5cis60°$ y $Z_5 = 11e^{4i}$
  calcula $\sqrt[3]{z_2 \cdot z_4 + \frac{z_1}{\overline{z_3} - z_5}}$
  \begin{align*}
    \sqrt[3]{z_2 \cdot z_4 + \frac{z_1}{\overline{z_3} - z_5}} &= \sqrt[3]{-10 - 4i \cdot 5cis60 + \frac{8(cos55° + isen55)}{\overline{2cis73°} - 11e^{4i}}} \\
    &= \sqrt[3]{10.77cis21.801 \cdot 5cis60 + \frac{8cis55}{\overline{0.584 + i1.912} + 7.19 + i8.324)}} \\
    &= \sqrt[3]{53.849cis81.801 + \frac{8cis55}{0.584 - i1.912 + 7.19 + i8.324)}} \\
    &= \sqrt[3]{53.849cis81.801 + \frac{8cis55}{7.776 - i6.412}} \\
    &= \sqrt[3]{53.849cis81.801 + \frac{8cis55}{10.078cis39.508}} \\
    &= \sqrt[3]{53.849cis81.801 + 0.793cis15.492} \\
    &= \sqrt[3]{31.766 + i43,481 + 0.611 + i0.504} \\
    &= \sqrt[3]{32.377 + i43.985} \\
    &= \sqrt[3]{64.616cis53.643} \\
    &= 64.616^{\frac{1}{3}}cis\frac{1}{3}\cdot53.643 \\
    &= 4.012cis17.881 \\
    &= 4.012(cos17.881 + isen17.881)
  \end{align*}

\item Sean $Z_1 = 9cis30°$, $Z_2 = 3 + 5i $, $ Z_3 = (4, -6)$, $ Z4 = 8cis45° $, $ Z_5 = 2(cos25° + i·sen25°)$
  Encontrar $Z$ en forma exponencial, para que se cumpla la siguiente igualdad:
  $\frac{z_3 + z \cdot z_1}{\overline{z_4}} - 7z_2 = (z_5)^2$
  \begin{align*}
    (\frac{z_3 + z \cdot z_1}{\overline{z_4}})^4 - 7z_2 = (z_5)^2 &= (\frac{(4,-6) + z \cdot 9cis30}{\overline{8cis54}})^4 - 7(3+5i) = (2cis25)^2 \\
    &= (\frac{7.211cis303.69 + z \cdot 9cis30}{\overline{8cis54}})^4 - 21+35i = 4cis50 \\
    &= (\frac{7.211cis303.69 + z \cdot 9cis30}{\overline{8cis54}})^4 - 21+35i = 4cis50 \\
    &= (\frac{7.211cis303.69 + z \cdot 9cis30}{4.702 - 6.472})^4 - 21+35i = 4cis50 \\
    &\implies (\frac{7.211cis303.69 + z \cdot 9cis30}{4.702 - 6.472})^4 = 23,571 - i32.428 \\
    &\implies \frac{7.211cis303.69 + z \cdot 9cis30}{4.702 - 6.472}= \sqrt[4]{40.088cis306.012} \\
    &\implies \frac{7.211cis303.69 + z \cdot 9cis30}{4.702 - 6.472}= 2.516cis76.503 \\
    &\implies 7.211cis303.69 + z \cdot 9cis30 = 7.999cis305.998 \cdot 2.516cis76.503 = 20.125cis22.501 \\
    &\implies z = \frac{20.125cis22.501 - 7.211cis303.69}{ 9cis30}\\
    &\implies z = \frac{14.593 + i13.701}{9cis30}\\
    &\implies z = \frac{20.016cis43,194}{9cis30}\\
    &\implies z = 2,224cis13.194 = 2.224e^{i0.23}\\
  \end{align*}


\end{itemize}
\end{document}

