# Problem 51

function isprime(n::Int)
    if n == 2 || n == 3
        return true
    elseif n % 2 == 0 || n % 3 == 0 || n <= 1
        return false
    end

    for i in 5:6:n
        if i^2 > n
            return true
        elseif n % i == 0 || n % (i+2) == 0
            return false
        end
    end
    true    
end

digs(x) =  (reverse âˆ˜ digits)(x)

function prime_replacement(n)
    dgs = digs(n)
end

@show digs(12345)
